{
	"name": "@cvix/assets",
	"version": "3.0.4",
	"private": true,
	"type": "module",
	"description": "Shared assets for all CVIX frontend applications",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./public/*": "./public/*",
		"./images/*": "./src/images/*",
		"./logos/*": "./src/logos/*",
		"./icons/*": "./src/icons/*",
		"./*": "./src/*"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./src/*"
			]
		}
	},
	"files": [
		"public",
		"src"
	],
	"devDependencies": {
		"@cvix/tsconfig": "workspace:*"
	}
}
